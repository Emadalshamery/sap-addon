{
  "manifest_version": 2,
  "name": "SAP Addon",
  "version": "1.3",
  "description": "*.wdf.sap.corp",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.wdf.sap.corp/*"
      ],
      "js": ["sap-addon.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "SAP Addon",
    "default_popup": "popup/popup.html"
  }
}